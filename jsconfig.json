{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
    }
  }
}